<script type="text/javascript">
function today_validation(){

	
	if(document.addReaches.firstName.value == 'Name'){
		document.getElementById("err_Name").innerHTML = "<span class='errorMessageText'>Please Enter Name</span>";
		return false;		
	}else{
		document.getElementById("err_Name").innerHTML = "";
	}
	
	
	if(document.addReaches.phoneNo.value != 'Phone Number'){

		var valid_num = checkNumber(document.addReaches.phoneNo.value);
		if( valid_num != true ) { 
			document.getElementById("err_phoneNo").innerHTML = "<span class='errorMessageText'>"+valid_num+"</span>";
			return false;
		}else{
			document.getElementById("err_phoneNo").innerHTML = "";
		}
	}
	
	if(document.addReaches.emailAddress.value == 'Email Id'){
		document.getElementById("err_emailAddress").innerHTML = "<span class='errorMessageText'>Please Enter Email Address</span>";
		return false;		
	}else{
		var email = document.addReaches.emailAddress.value;
		
			var re_mail = /^([a-zA-Z0-9_\.\-])+\@(([a-zA-Z0-9\-])+\.)+([a-zA-Z])+$/;
			var rslt = re_mail.test(email);
			if (rslt) { 
			document.getElementById("err_emailAddress").innerHTML = "";
			}else { 	
			document.getElementById("err_emailAddress").innerHTML = "<span class='errorMessageText'>Please Enter Valid E-mail Address</span>";
			return false;
			}
	}
	
	
	if(document.addReaches.message.value == 'Comments'){
		document.getElementById("err_message").innerHTML = "<span class='errorMessageText'>Please Enter Comment</span>";
		return false;		
	}else{
		document.getElementById("err_message").innerHTML = "";
	}
	
	return true;
}

function checkNumber(name)  {
	var re_alpha =/^[0-9 .]+$/;
	var rslt = re_alpha.test(name);
	if (rslt) { ret = rslt; }
	else { ret = 'Please Enter Valid Number'; }
	return ret;
}

</script>

