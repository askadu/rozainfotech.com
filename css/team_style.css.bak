@charset "utf-8";
/* CSS Document */
article, aside, audio, canvas, command, datalist, details, embed, figcaption, figure, footer, header, hgroup, keygen, meter, nav, output, progress, section, source, video {
	display: block;
}
mark, rp, rt, ruby, summary, time {
	display: inline
}
/* Global properties ======================================================== */
body {
	margin: 0 auto;
	padding: 0;
	font-family:Verdana, Arial, Helvetica, sans-serif;
	position:relative;
	height:100%;
}
html {
	min-width:1000px;
}
html, body {
	height:100%;
}
img {
	border:0;
}
li {
	list-style:none;
}
p {
	font-family:Verdana, Arial, Helvetica, sans-serif;
}
strong{
	float:left;
	padding:5px 0px !important;
	font-size:13px !important;
	width:100%;
}
@font-face {
 font-family: 'Georgia';
 src: url(../fonts/ufonts.com_georgia.html);
 src: local('Georgia'), url(../fonts/ufonts.com_georgia.html) format('truetype');
}

@font-face {
   font-family: "Univers-Condensed";
   src: url("../font/Univers%20Light%20Condensed.html") format("truetype");
    font-weight: normal;
 	font-style: normal;
}

/* Global Structure ============================================================= */
.container {
	margin: 0 auto;
	position: relative;
	width: 1000px;
	font-size:.8125em;
}
/* Header */
	header {
	height:108px;
	overflow:hidden;
	background:url(../images/team_bg.jpg);
	background-repeat:repeat;
	width:100%;
}
/*Banner*/
	#banner {
	height:543px;
	background:url(../images/banner_bg.jpg);
	background-repeat:no-repeat;
	overflow:hidden;
}
/*Gallery*/
	#gallery {
	background:url(../images/team_bg.jpg);
	background-repeat:repeat;
	overflow:hidden;
}
/* Content */
	#content {
	float: left;
	width:1000px;
}
/* content_index */
	#content_home {
	float: left;
	width:64%;
	margin-left:4px;
}
/* content_inner */
	#content_inner {
	float: left;
	width:73%;
}
/* Content_about */
	#content_about {
	float: left;
	width:73%;
	margin:0px 2px 0px 0px;
}
/* Content_Portfolio */
	#content_port {
	float: left;
	width:990px;
	margin:0px 13px 0px 0px;
}
/* Footer */
	footer {
	padding:20px 0 0px 0;
	border-top:5px solid #eee;
	background:url(../images/footer.jpg);
	background-repeat:repeat-x;
	width:100%;
	height:270px;
}
/* Left & Right alignment */

.fleft {
	float:left;
}
.fright {
	float:right;
}
.clear {
	clear:both;
}
.alignright {
	text-align:right;
}
.aligncenter {
	text-align:center;
}
.wrapper {
	width:100%;
	overflow:hidden;
}
/* The inside class provides consistent padding. To be used often! */
.inside {
	padding:15px 0px 10px 0px;
}
.inside_inner {
	padding:10px 20px 10px 26px;
	z-index:-2;
	float:left;
}
/*----- form defaults -----*/
input, select, textarea {
	font-family:Arial, Helvetica, sans-serif;
	font-size:1em;
	vertical-align:middle;
	font-weight:normal;
}
/*----- other -----*/
figure {
	margin-bottom:18px;
}
.img-indent {
	margin:0 20px 0 0;
	float:left;
}
.img-box {
	width:100%;
	overflow:hidden;
	padding-bottom:15px;
}
.img-box figure {
	float:left;
	margin:0 20px 0 0;
	padding:0;
}
.extra-wrap {
	overflow:hidden;
}
p {
	margin-bottom:18px;
}
.p1 {
	margin-bottom:9px;
}
.p2 {
	margin-bottom:18px;
}
.p3 {
	margin-bottom:27px;
}
/*----- txt, links, lines, titles -----*/
a {
	color:#f27f02;
	outline:none;
}
a:hover {
	text-decoration:none;
}
h2 {
	font-size:30px;
	line-height:1.2em;
	font-weight:normal;
	color:#212222;
	margin-bottom:22px;
}
h2 span {
	color:#8a8a8a;
}
h3 {
	font-size:1em;
	margin-bottom:6px;
}
h3 a {
	color:#242424;
}
h3 a:hover {
	text-shadow: 1px 1px 1px #888;
}
