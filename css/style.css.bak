#read_more1,#read_more2,#read_more3,#read_more4 {     
	background-attachment: scroll;
    background-clip: padding-box !important;
	background-image:  -moz-linear-gradient(top, #53ace6, #1255a4); /* FF3.6 */
	background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #53ace6), color-stop(1, #1255a4)); /* Saf4+, Chrome */
    filter:  progid:DXImageTransform.Microsoft.gradient(startColorStr='#53ace6', EndColorStr='#1255a4'); /* IE6,IE7 */
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorStr='#53ace6', EndColorStr='#1255a4')"; /* IE8 */
    background-origin: padding-box;
    background-position: 0 0;
    background-repeat: repeat;
    background-size: auto auto;
    border: 1px solid #1255A4;
    -moz-border-radius:5px;
	-webkit-border-radius:5px;
	border-radius:5px;
    box-shadow: 0 1px 8px #888888;
    color: #000000;
    cursor: pointer;
    display: inline-block;
	font-family:"Myriad Pro";
    float: right;
    font-size: 15px;
    font-weight: normal;
    line-height: 1em;
    margin: 0 5px 10px 0;
    padding: 6px 18px;
    text-decoration: none;
}
#read_more1,#read_more2,#read_more3,#read_more4 {     
    background-clip: padding-box !important;
	background-image:  -moz-linear-gradient(top, #53ace6, #1255a4) !important;
	background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #53ace6), color-stop(1, #1255a4)) !important;
    filter:  progid:DXImageTransform.Microsoft.gradient(startColorStr='#53ace6', EndColorStr='#1255a4') !important;
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(startColorStr='#53ace6', EndColorStr='#1255a4')" !important;
    border: 1px solid #1255A4;
    -moz-border-radius:5px;
	-webkit-border-radius:5px;
	border-radius:5px;
    box-shadow: 0 1px 8px #888888;
    color: #000000;
    cursor: pointer;
    display: inline-block;
	font-family:"Myriad Pro";
    float: right;
    font-size: 15px;
    font-weight: normal;
    line-height: 1em;
    margin: 0 5px 10px 0;
    padding: 6px 18px;
    text-decoration: none;
}
#read_more1 a,#read_more2 a,#read_more3 a,#read_more4 a { color: #FFFFFF; text-decoration: none; }
#read_more1 a:hover,#read_more2 a:hover,#read_more3 a:hover,#read_more4 a:hover { color: #000000; text-decoration: none; }


#read_more1{ left:450px; top:290px;}
#read_more4{ left:450px; top:290px;}
#read_more2{ left:180px; top:290px;}
#read_more3{ left:700px; top:290px;}

